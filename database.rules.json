{
  "rules": {

    ".read": "auth != null",
    ".write": "auth != null",
    "Listen": {
      ".read": "auth != null",
      ".write": "auth != null",
      ".options": {
        "cors": {
          "origin": ["http://localhost:3000"],
          "methods": ["GET", "POST"],
          "headers": ["Content-Type", "Authorization"]
        }
      }
    }
  }
}
